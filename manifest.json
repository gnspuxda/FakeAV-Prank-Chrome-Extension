{
  "name": "Universal Browser Defender - Online Smart Antivirus",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["tabs"],
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*", "*://*.imdb.com/*", "*://*.google.com/*", "*://*.bing.com/*", "*://*.play.google.com/*", "*://*.amazon.com/*", "*://*.microsoft.com/*"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}